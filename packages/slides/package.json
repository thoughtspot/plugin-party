{
  "name": "slides",
  "version": "0.0.0",
  "main": "dist/index.js",
  "devDependencies": {
    "@google/clasp": "^2.4.2",
    "@preact/preset-vite": "^2.5.0",
    "@types/google-apps-script": "^1.0.57",
    "@vitejs/plugin-basic-ssl": "^1.0.1",
    "npm-run-all": "^4.1.5",
    "rollup-plugin-visualizer": "^5.9.0",
    "sass": "^1.58.0",
    "vite": "^4.1.0"
  },
  "scripts": {
    "build": "vite build",
    "tsc": "tsc -p .",
    "tsw": "tsc -p . -w",
    "claspw": "clasp push -w",
    "clasp": "clasp push",
    "dist:dev": "npm-run-all -p tsw copy-index claspw",
    "dist": "npm-run-all -s tsc copy-index",
    "deploy": "HOST=$SLIDES_HOST npm-run-all -p build dist",
    "lint": "eslint src/**",
    "start": "HOST=https://localhost:5173 npm-run-all -p dist:dev vite",
    "vite": "vite",
    "copy-index": "gsuite-shell"
  },
  "dependencies": {
    "build-utils": "0.0.0",
    "gsuite-shell": "0.0.0",
    "i18n": "0.0.0",
    "preact": "^10.11.13",
    "preact-router": "^4.1.0",
    "ts-init": "0.0.0",
    "widgets": "0.0.0"
  }
}
